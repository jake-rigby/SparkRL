<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 rollOver="{currentState='over'}" rollOut="{currentState='normal'}" width="100%" >
	<fx:Declarations>
		<fx:String id="text"/>
	</fx:Declarations>
	<s:states>
		<s:State name="normal"/>
		<s:State name="over"/>
	</s:states>
	<s:Rect width="100%" height="100%">
		<s:fill>
			<s:SolidColor color.normal="{getStyle('fillNormal')}" color.over="{getStyle('fillOver')}"/>
		</s:fill>
	</s:Rect>
	<s:Label paddingBottom="5" paddingLeft="5" paddingRight="5" paddingTop="5" text="{text}"
			 color.normal="{getStyle('textNormal')}" color.over="{getStyle('textOver')}"/>
</s:Group>
